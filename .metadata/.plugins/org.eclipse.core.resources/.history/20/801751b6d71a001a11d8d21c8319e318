package demo;

public class Stringhom {

	public static void main(String[] args) {
		String S1="Whats a matter son your notebooks are still uncovered";
		System.out.println(S1);
		int size=0;
		int index=0;
		for(int i=0;i<=S1.length();i++)
		{
			if(S1.charAt(i) == 's') {
			size++;	
		 index=S1.indexOf('s',i);
		 System.out.println("Index of S "+index);
		 
			}
		}
		
		System.out.println("Number of time S char occur "+size);
		// TODO Auto-generated method stub

	}

}
 